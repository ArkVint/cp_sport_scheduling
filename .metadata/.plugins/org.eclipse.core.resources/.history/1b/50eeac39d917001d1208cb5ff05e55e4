using CP;
int nbTeams = ...;
range teams = 1..nbTeams;
int nbRounds = ...;
int M[teams][teams]=...;

dvar int x[teams][teams][0..nbRounds+1] in 0..1;

subject to
{
	forall(i,j in teams)
		sum(r in 1..nbRounds) x[i][j][r] == M[i][j];

}